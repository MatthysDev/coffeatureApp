{
  "expo": {
    "name": "coffeatureApp",
    "slug": "coffeatureApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/shiba.png",
    "splash": {
      "image": "./assets/img/shiba.png",
      "resizeMode": "contain",
      "backgroundColor": "#F78965"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/shiba.png",
        "backgroundColor": "#F78965"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
